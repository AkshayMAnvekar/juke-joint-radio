<table border="0" style="width: 100%;">
  {% for song in pending %}
    <tr>
      <td>{{ song.name }}</td>
      <td>{{ song.duration }}</td>
    </tr>
  {% endfor %}
  {% for song in queued %}
    <tr>
      <td>{{ song.name }}</td>
      <td>{{ song.duration }}</td>
    </tr>
  {% endfor %}
</table>
